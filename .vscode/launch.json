    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch localhost with sourcemaps",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:1234",
            "webRoot": "${workspaceRoot}/dev",
            "userDataDir": false,
        },
        {
            "name": "Attach localhost with sourcemaps with temp profile",
            "type": "chrome",
            "request": "attach",
            "url": "http://localhost:1234",
            "webRoot": "${workspaceRoot}/dev",
        },
    ]
}